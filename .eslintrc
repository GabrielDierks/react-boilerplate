{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "indent": [
      "error",
      4,
      {
        "SwitchCase": 1
      }
    ],
    "react/jsx-indent": [
      2,
      4
    ],
    "react/jsx-indent-props": [
      2,
      4
    ],
    "react/jsx-filename-extension": [
      2,
      {
        "extensions": [
          ".js"
        ]
      }
    ],
    "react/prefer-stateless-function": [
      0,
      {}
    ],
    "react/forbid-prop-types": [
      0,
      {}
    ],
    "jsx-a11y/no-static-element-interactions": [
      0,
      {}
    ]
  }
}
